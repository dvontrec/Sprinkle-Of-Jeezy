<% include ./partials/header %>

	<h1 class="form-label">Log IN</h1>
	<form action="/login" method="POST" class="form login-form">
		<div class="login-form">
			<div class="form-group">
				<label for="username">Username</label>
				<input tabindex="1" type="text" name="username" placeholder="username">
			</div>
			<div class="form-group">
				<label for="password">Password</label>
				<input tabindex="2" type="password" name="password" placeholder="password">
			</div>
			<div class="form-group">
				<button tabindex="3">Log in</button>
			</div>
		</div>
	</form>
<% include ./partials/footer %>